{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "Percentage of population with Highest education being Year 12",
    "width": 800,
    "height": 400,
    "projection": {"type": "equalEarth"},
    "data": {
      "url": "https://raw.githubusercontent.com/tvog0001/3179_Vis2/blob/main/STE_2021_AUST_GDA2020.json",
      "format": {"type": "topojson", "feature": "STE_2021_AUST_GDA2020"}
    },
    "transform": [
      {
        "lookup": "properties.STE_NAME21",
        "from": {
          "data": {
            "url": "https://raw.githubusercontent.com/tvog0001/3179_Vis2/blob/main/HighestPopulationHighSchoolByState.csv"
          },
          "key": "State",
          "fields": ["Active"]
        }
      }
    ],
    "mark": {"type": "geoshape"},
    "encoding": {
      "color": {
        "field": "Percentage whos highest education is secondary school",
        "type": "quantitative"
      },
      "tooltip": [
        {"field": "properties.STE_NAME21", "type": "nominal", "title": "State"},
        {"field": "Active", "type": "quantitative"}
      ]
    }
  }